<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="de.cau.inf.se.sopro.ui.home.HomeFragment"
        android:label="project_label"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_navigation_home_to_navigation_project_overview"
            app:destination="@id/navigation_project_overview"
            />
        </fragment>

    <fragment
        android:id="@+id/navigation_project_overview"
        android:name="de.cau.inf.se.sopro.ui.projectoverview.ProjectOverviewFragment"
        android:label="project_overview_label"
        tools:layout="@layout/fragment_project_overview"


        />


</navigation>